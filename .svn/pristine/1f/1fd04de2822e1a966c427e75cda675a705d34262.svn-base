.order-num,
.logistics{
	border-top: 1px solid #F7F7F7;
	border-bottom: 10px solid #F7F7F7;
	padding: 15px;
	position: relative;
}
.order-num .num{
	display: block;
	font-size: 14px;
}
.img_ljm{
	width: 54px;
	height: 13px;
	
}
.order-num .city{
	margin-top: 20px;
	display: flex;
}
.order-num .city .status{
	width: 60px;
	margin: 0 10px;
	position: relative;
}
.order-num .city .status image{
	width: 100%;
	vertical-align: middle;
}
.order-num .city .status text{
	font-size: 10px;
	position: absolute;
	width: 100%;
	top: -8px;
	text-align: center;
	color: #999999;
}
.order-num .relation{
	color: #666666;
	margin-top: 12px;
	font-size: 12px;
}
.order-num .arrive-time{
	position: absolute;
	top: 15px;
	right: 15px;
	font-size: 12px;
	text-align: right;
}
.order-num .arrive-time text{
	color: #FF0000;
	font-size: 13px;
	font-weight: bold;
	display: block;
}
.logistics .title{
	display: block;
	font-size: 16px;
	margin-bottom: 15px;
}
.logistics .item-list{
	position: relative;
	display: flex;
}
.logistics .item-list .status{
	width: 60px;
	color: #999999;
	margin-right: 8px;
	display: block;
}
.logistics .item-list .info{
	width: calc(100% - 68px);
	width: -webkit-calc(100% - 68px);
	display: flex;
}
.logistics .item-list image{
	width: 12px;
	height: 66px;
	margin-right: 10px;
}
.logistics .item-list .info > view{
	width: calc(100% - 22px);
	color: #999999;
	line-height: 18px;
}
.logistics .item-list .info .time{
	display: block;
	font-size: 12px;
	margin-top: 5px;
}
.user-info{
	margin: 15px;
	border-bottom: 1px solid #F5F5F5;
}
.user-info .user{
	font-size: 16px;
	line-height: 22px;
}
.user-info .user .iconfont{
	font-size: 22px;
	float: left;
	color: #2970DD;
}
.user-info .user .iconfont.icon-shou{
	color: #EF9A42;
}
.user-info .user .name{
	padding: 0 8px;
}
.user-info .company-name{
	margin: 3px 0 3px 28px;
}
.user-info .address{
	margin-left: 28px;
	color: #999999;
	margin-bottom: 15px;
	font-size: 12px;
}
.order-detail{
	padding: 0 10px;
	padding-bottom: 80px;
}
.order-detail .label{
	margin-left: 5px;
	width: calc(50% - 5px);
	display: inline-block;
	vertical-align: text-top;
	margin-bottom: 8px;
	line-height: 20px;
	color: #666666;
	font-size: 12px;
	position: relative;
}
.order-detail .label.row{
	width: calc(100% - 5px);
	display: block;
}
.order-detail .label .s-t{
	display: block;
	position: absolute;
	top: 0;
	left: 0;
}
.order-detail .label .txt{
	margin-left: 58px;
	font-size: 12px;
	word-wrap: break-word;
	display: flex;
	align-items: center;
}
.order-detail .label .icon--right-jian{
	font-size: 12px;
}
.order-detail .label .iconfont{
	color: #999999;
	margin-left: 5px;
}
.order-detail .label .orange{
	color: #fd9523;
}
.market{
	text-decoration: line-through;
	color: #999999;
	display: block;
}
.signing-pictures{
	font-size: 12px;
	color: #4285F4;
	display: flex;
	align-items: center;
	width: 40px;
}
.signing-pictures .iconfont{
	padding-right: 3px;
}
.footer{
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: #FFFFFF;
	border-top: 1px solid #F5F5F5;
}
.footer button{
	display: inline-block;
	background-color: #FFFFFF;
	border: 1px solid #4285F4;
	color: #4285F4;
	border-radius: 30px;
	padding: 0 15px;
	height: 35px;
	line-height: 33px;
	margin: 10px 10px 10px 0;
	float: right;
}
.footer button .iconfont{
	padding-right: 5px;
}
.scroll-wrapper{
	max-height: 300px;
	overflow-y: auto;
}
.none{
	color: #999999;
}
.printBtn{
	/* background-color: #4285F4;
	color: #FFFFFF;
	width: 60px;
	line-height: 30px;
	text-align: center;
	margin: 15px 5px;
	border-radius: 3px; */
	color: #4285F4;
}
.print-con{
	background: #FFFFFF;
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 1000;
	color: #fff;
	text-align: center;
	padding-top: 10px;
}
.bar-code{
	position: absolute;
	top: 25%;
	left: 10px;
	right: 10px;
	
	background-color: #FFFFFF;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	border-radius: 5px;
	z-index: 10001;
}
.bar-code > canvas {
    width: 259px;
    height: 75px;
	margin-bottom: 10px;
}
.bar-code > view{
	font-size: 18px;
	font-weight: bold;
	letter-spacing: 5px;
}