.qy_img{
	width:18px;
	height:15px;
	vertical-align: middle;
	margin-right:3px;
}
.right_v{
	display: flex;
}
.search{
	padding: 8px 15px;
	background-color: #fff;
	height: 34px;
}
.search .label-input{
	display: flex;
	display: -webkit-flex;
	align-items: center;
	justify-content: space-between;
	float: left;
	background-color: #f7f7f7;
	width: calc(100% - 65px);
	width: -webkit-calc(100% - 65px);
	line-height: 34px;
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
	padding-right: 10px;
	overflow: hidden;
}
.search .label-input input{
	width: 80%;
	margin-left: 10px;
}
.search button{
	display: inline-block;
	background: #f7f7f7;
	width: 50px;
	padding: 0;
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
	float: right;
}
.tab-nav{
	display: flex;
	display: -webkit-flex;
	align-items: center;
	height: 43px;
	background-color: #fff;
}
.tab-nav > view{
	width: 50%;
	text-align: center;
}
.tab-nav > view.active text{
	border-bottom: 1px solid #4285F4;
	color: #4285F4;
	padding: 0 10px 10px;
	width: auto;
}
.tab-con{
	background-color: #f7f7f7;
	position: absolute;
	top: 93px;
	left: 0;
	right: 0;
	bottom: 0;
}
.order-item-list{
	padding: 15px;
	margin-top: 10px;
	background-color: #fff;
	position: relative;
}
.order-item-list .number{
	font-size: 12px;
	color: #999;
}
.order-item-list .iconfont{
	margin-left: 3px;
}
.order-item-list .detail{
	font-size: 14px;
	position: absolute;
	top: 15px;
	right: 15px;
}
.order-item-list .txt-list > view{
	font-size: 12px;
	color: #999999;
	padding-bottom: 3px;
}
.order-item-list .txt-list .line{
	border-bottom: 1px dashed #e8e8e8;
	padding-bottom: 7px;
	margin-bottom: 10px;
}
.order-item-list .info{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	align-items: center;
}
.order-item-list .info .price{
	font-size: 20px;
	color: #f00;
}
.order-item-list .info .btn{
	display: block;
	border-radius: 6px;
	background-color: #4285F4;
	color: #FFFFFF;
	padding: 4px 12px;
}
.order-item-list .info .btn.del{
	background-color: #f00;
}
.order-item-list .info .btn.ljm{
	background-color: #00aaff;
	margin-right: 10px;
}
.senior-search-popup{
	position: fixed;
	top: 50px;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 2;
}
.senior-search-popup .overlay{
	position: absolute;
	top:0;
	bottom: 0;
	left: 0;
	right: 0;
	background-color: rgba(0, 0, 0, 0.6);
}
.senior-search-popup .more-search{
	position: absolute;
	top:0;
	left: 0;
	right: 0;
	background-color: #fff;
	padding-left: 15px;
}
.senior-search-popup .more-search .name{
	font-size: 15px;
	display: block;
	margin-top: 10px;
}
.senior-search-popup .more-search .search-type-list{
	display: flex;
	display: -webkit-flex;
	justify-content: space-between;
	margin: 10px 0 15px;
	line-height: 34px;
	text-align: center;
	color: #999;
}
.senior-search-popup .more-search .search-type-list text,
.senior-search-popup .more-search .search-type-list .picker{
	width: 100%;
	height: 34px;
	background: #f7f7f7;
	border-radius: 4px;
	overflow: hidden;
	margin-right: 15px;
	color: #333;
}
.senior-search-popup .more-search .search-type-list .picker:first-child{
	margin-right: 10px;
}
.senior-search-popup .more-search .search-type-list .picker+.picker{
	margin-left: 10px;
}
.senior-search-popup .more-search .search-type-list text.active{
	background-color: #d6ebff;
	background-image: url(../../static/images/icon-active.png);
	background-repeat: no-repeat;
	background-size: 18px;
	background-position: bottom right;
}
.senior-search-popup .more-search .search-type-list .picker.active{
	border-bottom: 1px solid #0f84f3;
}
.senior-search-popup .more-search .search-type-list.btn{
	margin: 0;
	margin-left: -15px;
}
.senior-search-popup .more-search .search-type-list.btn button{
	width: 50%;
	height: 50px;
	line-height: 50px;
	padding: 0;
	background: #d6ebff;
	color: #0f84f3;
	border-radius: 0;
}
.senior-search-popup .more-search .search-type-list.btn button:last-child{
	background: #0f84f3;
	color: #fff;
}
.bar-code{
	position: absolute;
	top: 35%;
	left: 10px;
	right: 10px;

	background-color: #FFFFFF;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	border-radius: 5px;
	z-index: 1001;
}
.bar-code > canvas {
    width: 259px;
    height: 75px;
	margin-bottom: 10px;
}
.bar-code > view{
	font-size: 18px;
	font-weight: bold;
	letter-spacing: 5px;
}