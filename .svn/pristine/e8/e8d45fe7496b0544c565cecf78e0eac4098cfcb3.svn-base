.page-bg{
	padding-bottom: 0;
}
.item-list{
	background-color: #fff;
	border-bottom: 1px solid #EEEEEE;
	padding: 15px;
	display: flex;
	display: -webkit-flex;
	align-items: flex-start;
	line-height: 24px;
}
.user-info-list{
	background-color: #FFFFFF;
	
}
.picj_er{
	height:auto;
	width:100%;
	z-index:10;
}
.inp_add{
	z-index: 100;
	margin-top: 8px;
}
.con_vie{
	width:100%;
	height:50px;
	display: flex;
	flex-direction: column;
	position: relative;
}
.item-list.right{
	justify-content: space-between;
}
.item-list .iconfont{
	font-size: 24px;
}
.item-list .iconfont.icon-shou{
	color: #EF9A42;
}
.item-list image{
	width: 24px;
	height: 24px;
	display: block;
	float: left;
	padding-right: 3px;
}
.select-address{
	align-items: center;
}
.select-address .txt{
	margin-left: 10px;
}
.select-address .txt.checked{
	color: #757575;
}
.weight{
	display: flex;
	justify-content: space-between;
	border-radius: 4px;
	height: 34px;
	line-height: 34px;
	border: 1px solid #eee;
}
.weight .btn{
	display: block;
	width: 36px;
	background: #f7f7f7;
	font-size: 18px;
	text-align: center;
}
.user-info-list .con .s-t{
	    font-size: 16px;
	    font-weight: bold;
}
.user-info-list .con .iconfont{
	margin-top: 11px !important;
}
.con{
	position: relative;
}
.weight .btn.reduce{
	border-right: 1px solid #eee;
}
.weight .btn.add{
	border-left: 1px solid #eee;
}
.weight input{
	width: 40px;
	height: 34px;
	text-align: center;
	line-height: 36px;
	margin: 0 2px;
}
.weight .unit{
	padding-right: 10px;
}
.volume{
	display: flex;
	display: -webkit-flex;
	align-items: center;
	height: 25px;
	line-height: 25px;
}
.volume input{
	width: 40px;
	height: 25px;
	border: 1px solid #EEEEEE;
	border-right: 0;
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
	padding-left: 5px;
}
.volume .unit{
	border: 1px solid #EEEEEE;
	border-left: 0;
	border-top-right-radius: 4px;
	border-bottom-right-radius: 4px;
	color: #999999;
	padding-right: 5px;
}
.volume .f-h{
	padding: 0 3px;
	margin-top: 3px;
}
.volume.total{
	margin-top: 15px;
	float: right;
}
.volume.total input{
	margin-left: 5px;
	color: #FD9523;
	width: 70px;
	text-align: center;
}
.item-list.tip{
	text-align: justify;
} 
.tip{
	line-height: 16px;
	padding: 10px 15px;
}
.estimate-list{
	background-color: #fff;
	padding: 10px 15px;
}
/* #ifdef H5 */
.estimate-list{
	margin-bottom: 50px;
}
/* #endif */
.estimate-list .e-l-item{
	border: 1px solid #eee;
	border-radius: 5px;
	padding: 10px 15px;
	justify-content: space-between;
	margin: 10px 0 5px;
	position: relative;
	overflow: hidden;
}
.estimate-list .e-l-item,
.estimate-list .e-l-item .e-l-i-info{
	display: flex;
	display: -webkit-flex;
	align-items: center;
}
.estimate-list .e-l-item .e-l-i-info{
	width: 65%;
}
.estimate-list .e-l-item image{
	width: 32px;
	height: 32px;
	margin-right: 10px;
}
.estimate-list text{
	display: block;
}
.estimate-list .e-l-item .e-l-i-info .flex{
	display: flex;
	display: -webkit-flex;
	align-items: center;
}
.estimate-list .e-l-item .e-l-i-info .flex text{
	color: #999999;
	font-size: 12px;
	padding-left: 8px;
}
.estimate-list .received-time{
	padding-top: 5px;
}
.estimate-list .price{
	color: #ff8c04;
}
.estimate-list .price {
	text-align: right;
}
.estimate-list .price .num{
	font-size: 22px;
	display: inline;
}
.estimate-list .price .original{
	font-size: 12px;
	color: #999;
	text-decoration: line-through;
}
.estimate-list .memo{
	padding-bottom: 5px;
}
.tip,
.estimate-list .memo{
	color: #ff0000;
	font-size: 12px;
	text-align: right;
	display: block;
}
.estimate-list .e-l-item .special{
	position: absolute;
    top: 2px;
    right: -17px;
    background-color: #f00;
    color: #fff;
    -webkit-transform: rotate(36deg);
    transform: rotate(36deg);
    font-size: 12px;
    width: 57px;
    text-align: center;
}